<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study_jams.kaio.movies">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/movies_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MovieWarcraftActivity"
            android:parentActivityName="com.study_jams.kaio.movies.MainActivity">
        </activity>
        <activity android:name=".MovieBatmanVsSupermanActivity"
            android:parentActivityName="com.study_jams.kaio.movies.MainActivity">
        </activity>
        <activity android:name=".MovieExMachinaActivity"
            android:parentActivityName="com.study_jams.kaio.movies.MainActivity">
        </activity>
        <activity android:name=".MovieDeadpoolActivity"
            android:parentActivityName="com.study_jams.kaio.movies.MainActivity">
        </activity>
    </application>

</manifest>
